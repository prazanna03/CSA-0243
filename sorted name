#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
 char str[100][100],s[20];
 int i,j,n;
 clrscr();
 printf("enter the number of names:\n");
 scanf("%d",&n);
 printf("enter the names in any order:\n");
 for (i=0;i,n;i++)
 {
  scanf("%s",str[i]);
 }
 for (i=0;i,n;i++)
 {
  for(j=i+1;j<n;j++)
  {
   if(strcmp(str[i],str[j])>0)
   {
   strcmp(s,str[i]);
   strcmp(str[i],str[j]);
   strcmp(str[j],s);
   }
  }
 }
 printf("the sorted order names are:\n");
 for(i=0;i<n;i++)
 {
  printf("%s",str[i]);
 }
 return 0;
}
